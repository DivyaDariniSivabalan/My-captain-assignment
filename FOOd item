#include<stdio.h> 
 
int main() 
{
    int No;
 
    printf("Enter A no: ");
    scanf("%d", &No);
    
    if(No==1)
    {
        printf("Food item :Pizza\n");
        printf("Price:239");
    }
    else if(No==2)
    {
        printf("Food item:Burger\n");
        printf("Price:129");
    }
    else if(No==3)
    {
        printf("Food item:Pasta\n");
        printf("Price:179");
    }
    else if(No==4)
    {
        printf("French Fries\n");
        printf("99");
    }
    else if(No==5)

    {
        printf("Sandwitch\n");
        printf("149");
    }
    return 0;
}
